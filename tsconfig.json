{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es5", "dom"],
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "types": ["node", "cypress"],
      "typeRoots": ["./node_modules/@types", "./cypress/supports"],
      "paths": {
        "cypress-sql-server": ["./node_modules/cypress-sql-server"]
      },
    },
    "include": ["cypress/**/*.ts", "cypress.config.ts", "src/types/**/*.d.ts"],
    "module": "commonjs",
    "moduleResolution": "node",
    "preserveValueImports": false
  }
  